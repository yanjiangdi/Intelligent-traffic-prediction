    "f=file('all_result.txt', \"a+\")\n",
    "for i in range(0,7):\n",
    "    f2 = open('r'+str(i)+'.txt')\n",
    "    for line in f2:\n",
    "        s=line.split(';')\n",
    "        if len(s)==5  and  s[0]!='':\n",
    "            f.write(s[1]+'#'+s[2]+'#'+s[3]+'#'+s[4])\n",
    "    f2.close()\n",
    "f.close()\n",
    "\n",
